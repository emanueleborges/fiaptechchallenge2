# Configurações AWS
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# S3 Configuration
S3_BUCKET_NAME=bovespa-pipeline-bucket
S3_RAW_DATA_PREFIX=raw-data/bovespa/
